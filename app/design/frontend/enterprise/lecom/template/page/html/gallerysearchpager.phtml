/**
 * Pager template
 *
 * @see Mage_Page_Block_Html_Pager
 */

$currentUrl = $this->helper('core/url')->getCurrentUrl();
?>aaaaaaaaaaaaaaaaaaaaaaasasssssssssssssssssssssssssdadas
<?php if($this->getCollection()->getSize()): ?>

    <?php if($this->getLastPageNum()>1): ?>
    <div class="pagelist">
        <ul class="num">
			<?php if (!$this->isFirstPage()): ?>
				<li>
					<div class="prev-page"><a class="previous<?php if(!$this->getAnchorTextForPrevious()): ?> i-previous<?php endif;?>" href="<?php echo $this->getPreviousPageUrl() ?>" title="<?php echo $this->__('Previous Page') ?>">Previous Page</a></div>
					<div class="lftArw">
						<a class="previous<?php if(!$this->getAnchorTextForPrevious()): ?> i-previous<?php endif;?>" href="<?php echo $this->getPreviousPageUrl() ?>" title="<?php echo $this->__('Previous') ?>">
					<?php if(!$this->getAnchorTextForPrevious()): ?>
							<img src="<?php echo $this->getSkinUrl('images/lftArw.gif') ?>" alt="<?php echo $this->__('Previous') ?>" class="v-middle" />
					<?php else: ?>
						<?php echo $this->getAnchorTextForPrevious() ?>
					<?php endif;?>
						</a>
					</div>
				</li>
			<?php endif;?>
			<?php foreach ($this->getFramePages() as $_page): ?>
				<?php if ($this->isPageCurrent($_page)): ?>
					<li class="active"><?php echo $_page ?></li>
				<?php else: ?>
					<li>
						<a href="<?php echo $this->getPageUrl($_page); ?>"><?php echo $_page ?></a>                   
					</li>
				<?php endif;?>
			<?php endforeach;?>

			<?php if (!$this->isLastPage()): ?>
				<li>					
					<div class="rgtArw">
							<a class="next<?php if(!$this->getAnchorTextForNext()): ?> i-next<?php endif; ?>" href="<?php echo $this->getNextPageUrl(); ?>" title="<?php echo $this->__('Next') ?>">
							<?php if(!$this->getAnchorTextForNext()): ?>
								<img src="<?php echo $this->getSkinUrl('images/rgtArw.gif') ?>" alt="<?php echo $this->__('Next') ?>" class="v-middle" />
							<?php else: ?>
								<?php echo $this->getAnchorTextForNext() ?>
							<?php endif;?>
						</a>
					</div>
					<div class="nxt-page"><a class="next<?php if(!$this->getAnchorTextForNext()): ?> i-next<?php endif; ?>" href="<?php echo $this->getNextPageUrl(); ?>" title="<?php echo $this->__('Next') ?>">Next Page</a></div>
				</li>
			<?php endif;?>
        </ul>
    </div>
    <?php endif; ?>
<?php endif ?>
